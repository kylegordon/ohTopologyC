[
    {   "name": "ohNet",
        "type": "openhome",
        "platform-specific": true,
        "version": "1.0.899",
        "archive-suffix": "-${debugmode}",
        "main-dir": "${dest}${name}-${platform}-${debugmode}",
        "configure-args": [
            "--ohnet-lib-dir", "${main-dir}/lib",
            "--ohnet-include-dir", "${main-dir}/include/ohnet"
        ],
        "_comment": [
            "See ohos for more examples, and ohdevtools/dependencies.py for details",
            "of the predefined values and expansion rules."
        ]
    },
    {   "name": "ohWafHelpers",
        "type": "openhome",
        "platform-specific": false,
        "version": "0.0.47",
        "archive-filename": "${name}-${version}.tar.gz"
    },
    {   "name":             "testharness",
        "version":          "0.1.23",
        "type":             "internal",
        "archive-filename": "${name}-${version}.tar.gz",
        "dest":             "dependencies/AnyPlatform/"
    },
    {   "name": "libplatform",
        "type": "internal",
        "version": "0.0.76",
        "archive-suffix": "-Release",
        "platform-ignore": {
            "Core-armv5": false,
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore": "${platform-ignore[$platform]}"
    },
    {   "name":             "libosa",
        "type":             "internal",
        "version":          "0.0.166",
        "archive-suffix":   "-Release",
        "platform-ignore": {
            "Core-armv5": false,
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore":           "${platform-ignore[$platform]}"
    },
    {   "name":             "transmute",
        "version":          "0.0.7",
        "type":             "internal",
        "archive-filename": "${name}-${version}.tar.gz",
        "platform-ignore": {
            "Core-armv5": false,
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore":           "${platform-ignore[$platform]}",
        "dest":             "dependencies/AnyPlatform/"
    },
    {   "name":             "volkano-tools",
        "version":          "0.0.4",
        "type":             "internal",
        "archive-filename": "${name}-${version}.tar.gz",
        "platform-ignore": {
            "Core-armv5": false,
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore":           "${platform-ignore[$platform]}",
        "dest":             "dependencies/AnyPlatform/${name}/"
    },
    {   "name": "openssl",
        "type": "external",
        "platform-specific": true,
        "external-version": "1.0.0d",
        "internal-version": "0.1.19",
        "archive-path": "http://core.linn.co.uk/~artifacts/artifacts/openssl/openssl-${external-version}-${internal-version}-${archive-platform}-${debugmode}.tar.bz2"
    },
    {   "name": "ohNetmon",
        "type": "internal",
        "platform-specific": true,
        "version": "0.0.4",
        "archive-suffix": "-${debugmode}",
        "main-dir": "${dest}${name}-${platform}-${debugmode}"
    }
]
